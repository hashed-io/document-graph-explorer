h1, h2, h3, h4, h5, h6, .text-h1, .text-h2, .text-h3, .text-h4, .text-h5, .text-h6
  font-family: "metropolis", "sans-serif" !default
  font-size: 18px

body
  font-family: 'metropolis', 'sans-serif' !default
.q-table tbody td
  font-size: 14px
  font-family: 'metropolis', 'sans-serif' !default
  background-color:#F3F4F6
.animated-icon
  transition: all 0.2s
.animated-icon:hover
  transform: scale(1.3)
@font-face{
  font-family: metropolis
  font-style normal
  font-weight: 500
  src: local(''),
       url('Fonts/Webfonts/WOFF2/Metropolis-Regular.woff2') format('woff2'), /* Super Modern Browsers */
       url('Fonts/Webfonts/WOFF/Metropolis-Regular.woff') format('woff'), /* Modern Browsers */
       url('Fonts/TrueType/Metropolis-Regular.ttf') format('truetype'), /* Safari, Android, iOS */
}
