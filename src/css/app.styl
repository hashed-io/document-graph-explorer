@font-face{
  font-family: metropolis
  font-style normal
  font-weight: 500
  src: local(''),
       url('Fonts/Webfonts/WOFF2/Metropolis-Regular.woff2') format('woff2'), /* Super Modern Browsers */
       url('Fonts/Webfonts/WOFF/Metropolis-Regular.woff') format('woff'), /* Modern Browsers */
       url('Fonts/TrueType/Metropolis-Regular.ttf') format('truetype'), /* Safari, Android, iOS */
}

h1, h2, h3, h4, h5, h6, .text-h1, .text-h2, .text-h3, .text-h4, .text-h5, .text-h6
  font-family: 'metropolis', 'sans-serif' !important
  font-size: 18px

body
  font-family: 'metropolis', 'sans-serif' !important
.q-table tbody td
  font-family: 'metropolis', 'sans-serif' !important
  font-size: 14px
  background-color:#F3F4F6
.animated-icon
  transition: all 0.2s
.animated-icon:hover
  transform: scale(1.3)
.btnTailwind
  border-radius: 10px
  height: 38px
  width: 108px
  font-weight: 600
  font-size: 14px
  color: white
  background-color: #4F46E5
.btnTailwind:hover
  box-shadow: blue
.btnTailWindLogin
  background: white;
  color: #4338CA
  height: 38px
  width: 108px
  font-weight: 600
  font-size: 10px
.btnTailWindLogout
  background: white;
  color: #4338CA
  border-radius: 10px
  height: 38px
  width: 108px
  font-weight: 600
  font-size: 14px
.btnTailwindIndex
  background:#4338CA
  color: white
  height: 50px
  width: 138px
  font-weight: 800
  font-size: 12px