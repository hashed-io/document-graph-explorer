@import url('https://rsms.me/inter/inter.css');
html { font-family: 'Inter', sans-serif; }
@supports (font-variation-settings: normal) {
  html { font-family: 'Inter var', sans-serif; }
}
img
  width : 10px !default
  height : 10px !default

h1, h2, h3, h4, h5, h6, .text-h1, .text-h2, .text-h3, .text-h4, .text-h5, .text-h6
  font-family: 'Inter var', 'sans-serif' !important
  font-size: 18px
  font-weight: 400
html
  font-family: 'Inter var', 'sans-serif' !important
  font-weight: 400
body
  font-family: 'Inter var', 'sans-serif' !important
  font-weight: 400
.q-table tbody td
  font-family: 'Inter var', 'sans-serif' !important
  font-weight: 400
  font-size: 14px
  background-color:#F3F4F6
.animated-icon
  transition: all 0.2s
.animated-icon:hover
  transform: scale(1)
  cursor pointer
.btnTailwind
  border-radius: 10px
  height: 38px
  width: 108px
  font-weight: 500
  font-size: 14px
  color: white
  background-color: $primary
.btnTailWindLogin
  background: white;
  color: $primary
  height: 38px
  width: 108px
  font-weight: 600
  font-size: 10px
.btnTailWindLogout
  background: white;
  color: $primary
  border-radius: 10px
  height: 38px
  width: 108px
  font-weight: 600
  font-size: 14px
// Button
.btn-login-wallet
  margin-top:10px
  border-radius:10px
  color: white
  background-color: $primary
.q-notification
  border-radius: 10px
  top:10px
.q-toggle__inner--truthy
  color: $primary !important
p
  margin: 0 !important
.q-chip__content
  font-weight: 800
  font-size: 12px
.q-item__section--side > .q-icon
  font-size: 15px !important
.anchor-link-active
  backdrop-filter: blur(4px)
.q-link
  border-radius: 10px !important
