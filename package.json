{
  "name": "document_explorer",
  "version": "0.0.2",
  "description": "Document Explorer",
  "productName": "Document Explorer",
  "cordovaId": "org.cordova.quasar.app",
  "author": "jmgayosso <jmgayosso@m1ghty.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev -m spa",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --fix --ext .js,.vue src",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "build": "quasar build -m spa",
    "test:unit": "ava",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:e2e": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:8080 \"cypress open\"",
    "test:e2e:ci": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:8080 \"cypress run\"",
    "serve:test:lighthouse": "quasar serve test/lighthouse/reports/ --port 8789",
    "audit:snyk": "snyk test --json > ./test/audits/snyk.json",
    "audit:node_modules": "yarn audit --json > ./test/audits/node_modules_audit.json",
    "audit:licenses": "nlf --summary detail > ./test/audits/licenses.txt",
    "audit:lighthouse": "lighthouse http://localhost:3000 view --output-path=./test/lighthouse/reports/index.html --config-path=./test/lighthouse/configs/full-config.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.{js,vue}": "yarn lint"
  },
  "dependencies": {
    "@quasar/cli": "^1.2.1",
    "@quasar/extras": "^1.10.10",
    "apollo-boost": "^0.4.9",
    "ava": "^1.4.1",
    "axios": "^0.18.1",
    "crypto-js": "^4.1.1",
    "cypress": "8.5.0",
    "date-diff": "^0.2.1",
    "dompurify": "^2.3.4",
    "dotenv": "^8.1.0",
    "eosjs": "^20.0.0",
    "eosjs-account-name": "^2.2.1",
    "eosjs-ecc": "^4.0.4",
    "google-libphonenumber": "^3.2.6",
    "graphql": "15.x",
    "i18n-iso-countries": "^4.3.1",
    "jdenticon": "^3.1.1",
    "marked": "^4.0.7",
    "nano-ipfs-store": "^0.1.3",
    "property-path": "^1.0.0",
    "quasar": "^1.15.23",
    "slugify": "^1.3.6",
    "ual-anchor": "^1.0.9",
    "universal-authenticator-library": "^0.1.4",
    "vue-analytics": "^5.20.1",
    "vue-croppa": "^1.3.8",
    "vue-i18n": "^8.0.0",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@quasar/app": "^1.9.6",
    "@quasar/quasar-app-extension-icon-genie": "^1.1.3",
    "@quasar/quasar-app-extension-testing": "^1.0.3",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^3.1.0",
    "@quasar/quasar-app-extension-testing-quality": "^1.0.0-beta.8",
    "@quasar/quasar-app-extension-testing-unit-ava": "^1.0.0-beta.12",
    "@quasar/quasar-app-extension-testing-unit-jest": "^2.2.2",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/vue": "^6.3.12",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "core-js": "^2.0.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "faker": "^5.5.3",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.1",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "style-loader": "^2.0.0",
    "vue-loader": "^15.9.8",
    "wallaby-vue-compiler": "^1.0.3"
  },
  "engines": {
    "node": "14.x"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
